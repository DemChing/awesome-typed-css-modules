{
  "name": "awesome-typed-css-modules",
  "version": "7.0.0",
  "description": "TypeScript type definition generator for Stylus/LESS/SASS/SCSS CSS Modules",
  "main": "dist/lib/main.js",
  "author": "Dem Ching",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/demching/typed-scss-modules.git"
  },
  "homepage": "https://github.com/demching/typed-scss-modules.git#readme",
  "keywords": [
    "scss",
    "sass",
    "less",
    "styl",
    "stylus",
    "sss",
    "sugarss",
    "css modules",
    "sass modules",
    "less modules",
    "stylus modules",
    "cli",
    "typescript",
    "type generator",
    "scss modules"
  ],
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "awesome-typed-css-modules": "ts-node ./lib/cli.ts",
    "typed-scss-modules": "ts-node ./lib/cli.ts",
    "clean": "rm -rf ./dist",
    "build": "yarn clean && tsc && chmod +x dist/lib/cli.js",
    "prepare": "yarn build && husky install",
    "check-types": "tsc --noEmit",
    "check-formatting": "prettier --check '**/*.{js,json,css,md,scss,tsx,ts}'",
    "commit": "commit"
  },
  "files": [
    "dist/lib"
  ],
  "bin": {
    "typed-scss-modules": "./dist/lib/cli.js",
    "awesome-typed-css-modules": "./dist/lib/cli.js"
  },
  "devDependencies": {
    "@babel/core": "^7.17.2",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@commitlint/prompt-cli": "^16.2.1",
    "@commitlint/travis-cli": "^16.2.1",
    "@types/css-modules-loader-core": "^1.1.0",
    "@types/glob": "^7.2.0",
    "@types/jest": "^27.4.0",
    "@types/less": "^3.0.3",
    "@types/node": "^17.0.18",
    "@types/node-sass": "^4.11.2",
    "@types/prettier": "^2.4.4",
    "@types/reserved-words": "^0.1.0",
    "@types/sass": "^1.43.1",
    "@types/stylus": "^0.48.36",
    "@types/yargs": "^17.0.8",
    "babel-jest": "^27.5.1",
    "babel-plugin-transform-import-meta": "^2.1.1",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "less": "^3.13.1",
    "lint-staged": "^12.3.4",
    "node-sass": "^7.0.1",
    "node-sass-json-importer": "^4.3.0",
    "postcss-nested": "4.2.3",
    "postcss-nested-5": "npm:postcss-nested@5",
    "prettier": "^2.5.1",
    "sass": "^1.49.7",
    "semantic-release": "^17.4.7",
    "stylus": "^0.54.8",
    "sugarss": "^1.0.1",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  },
  "peerDependencies": {
    "less": ">= 3.13.1",
    "node-sass": ">= 4.12.0",
    "sass": ">= 1.49.7",
    "stylus": ">= 0.54.8",
    "sugarss": ">= 1.0.1"
  },
  "peerDependenciesMeta": {
    "less": {
      "optional": true
    },
    "node-sass": {
      "optional": true
    },
    "sass": {
      "optional": true
    },
    "stylus": {
      "optional": true
    },
    "sugarss": {
      "optional": true
    }
  },
  "dependencies": {
    "@demching113/css-modules-loader-core": "^2.0.1",
    "bundle-require": "^3.0.4",
    "camelcase": "^6.3.0",
    "chalk": "4.1.2",
    "chokidar": "^3.5.3",
    "css-modules-loader-core": "^1.1.0",
    "esbuild": "^0.14.21",
    "glob": "^7.2.0",
    "joycon": "^3.1.1",
    "param-case": "^3.0.4",
    "reserved-words": "^0.1.2",
    "slash": "^3.0.0",
    "yargs": "^17.3.1"
  },
  "lint-staged": {
    "*.{js,json,css,md,scss,tsx,ts}": [
      "prettier --write"
    ]
  }
}
